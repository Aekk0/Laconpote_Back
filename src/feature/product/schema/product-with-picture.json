{
    "$id": "product-with-picture",
    "title": "product",
    "description": "A JSON Schema describing a Product",
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "nullable": false
      },
      "name": {
        "type": "string",
        "nullable": true
      },
      "description": {
        "type": "string",
        "nullable": true
      },
      "price": {
        "type": "number",
        "nullable": true
      },
      "pictures": {
        "type": "array",
        "items": {
            "$ref": "picture"
        }
      }
    }
  }
    